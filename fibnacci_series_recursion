#include <stdio.h>
int fibnacci(int n)
{
    if(n==0)
       return 0;
    else if(n==1)
       return 1;
    else
       return (fibnacci(n-1)+fibnacci(n-2));
}
int main() {
    // Write C code here
   int n,i;
   printf("enter the value of n:");
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       printf("%d ",fibnacci(i));
   }
    return 0;
}
