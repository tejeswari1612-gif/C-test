#include <stdio.h>

int main() {
    // Write C code here
   int n=5,i,j,val;
   for(i=0;i<n;i++)
   {
       val=i+1;
       for(j=0;j<n;j++)
       {
           if(i==0)
           {
               printf("%d",j+1);
           }
           else if(j<n-i)
           {
               printf("%d",j+val);
           }
           else
              printf("%d",n-j);
       }
       printf("\n");
   }
   

    return 0;
}
